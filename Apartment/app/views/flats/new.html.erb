<p>Add Flat</p>
<%= form_for ([@property, @property.flats.build]) do |f| %>
	<%= f.label :Owners_Name%>
	<%= f.text_field :name %>
	<br />
	<%= f.label :Apartment_number%>
	<%= f.text_field :flat_nr %>
	<br />
	<%= f.label :Caretaker%>
	<%= f.text_field :caretaker %>
	<br />
	<%= f.label :Content %>
    <%= f.text_field :content %>
    <br />
    
	<div class="actions">
    <%= f.submit %>
  </div>
<% end %>



<%= link_to 'Back', properties_path %>
